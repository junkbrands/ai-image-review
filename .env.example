# Image analysis provider: "claude" (default) or "google"
VISION_PROVIDER=claude

# Anthropic API key (required when VISION_PROVIDER=claude)
ANTHROPIC_API_KEY=sk-ant-your-key-here

# Google Cloud API key (required when VISION_PROVIDER=google)
# Enable the Cloud Vision API in your Google Cloud project first.
GOOGLE_CLOUD_API_KEY=sk-ant-your-key-here

# Port the server listens on (defaults to 3000)
PORT=3000
